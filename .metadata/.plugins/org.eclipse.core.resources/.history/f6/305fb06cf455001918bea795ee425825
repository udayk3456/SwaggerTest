package com.app.controller;

import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RestController;

@RestController
public class CrudType {
	@GetMapping("/get")
	public String get() {
		return "getType";
	}
	
	public String post() {
		return "postType";
	}
	public String put() {
		return "putType";
	}
	public String delete() {
		return "deleteType";
	}

}
